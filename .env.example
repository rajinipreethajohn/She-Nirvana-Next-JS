# Example environment file
# Supabase credentials
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Stripe credentials
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# App URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Product IDs
NEXT_PUBLIC_STRIPE_MONTHLY_PRICE_ID=price_monthly
NEXT_PUBLIC_STRIPE_ANNUAL_PRICE_ID=price_annual

# Email service (for newsletters)
EMAIL_SERVER_USER=your_email_server_user
EMAIL_SERVER_PASSWORD=your_email_server_password
EMAIL_SERVER_HOST=your_email_server_host
EMAIL_SERVER_PORT=587
EMAIL_FROM=noreply@shenirvana.com